config BUTTON_RESET
		bool "BUTTON_RESET"
		default n
		help
			Enable this option if you need reset button
	  
if 	BUTTON_RESET
config BUTTON_RESET_MODE
	int "BUTTON_RESET_MODE 0 for gpio ,1 for irq"
	default "0"
	help
	  Enable this 
	  
