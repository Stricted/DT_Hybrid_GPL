lib-$(CONFIG_ATP_PING)         += ping.o
lib-$(CONFIG_ATP_PING6)        += ping.o
lib-$(CONFIG_ATP_ARP)          += arp.o interface.o
lib-$(CONFIG_ATP_TRACEROUTE6)     += traceroute6.o
lib-$(CONFIG_ATP_DNSDIAG)   += dnsdiag.o