[settings]
functionname=hi_voip_cli_del_conference_user;  
attributenum=3;
attributeflag = 1 ;   //0:close 1:open

[parameter]
attribute=alias<chanid>       type<uint>   range<0x0,0xFFFFFFFF>   default<0>; /*通道号*/
attribute=alias<confid>       type<uint>   range<0x0,0xFFFFFFFF>   default<0>; /*会议ID*/
attribute=alias<direction>    type<uint>   range<0x0,0xFFFFFFFF>   default<1>; /*方向*/

[help]
usage: del_conference_user                 ; 删除会议室成员    
       del_conference_user -v chanid 0     ; 通道号
       del_conference_user -v confid 0     ; 会议ID
       del_conference_user -v direction 0  ; 方向
