[settings]
functionname  = hi_kphy_cmdtype_write_phy_mapping;  
attributenum  = 5;
attributeflag = 1 ;   //0:close 1:open

[parameter]
attribute=alias<index>          type<uint>      range<0x0,0xFFFFFFFF> 	default<4>;
attribute=alias<phyid>          type<uint>      range<0x0,0xFFFFFFFF> 	default<0x18>;
attribute=alias<vendor>         type<uint>      range<0x0,0xFFFFFFFF> 	default<1>;
attribute=alias<macindex>       type<uint>      range<0x0,0xFFFFFFFF> 	default<0>;
attribute=alias<devname>        type<string>    range<0x0,32>           default<nas1>;

[help]
typedef struct {
    hi_uint32 ui_index;    /* 管理ui_index */  
    hi_uint32 ui_phyid;    /* 物理phy id */
    hi_uint32 ui_vendor;   /* 0标识内部phy, 1~n标识外部扩展phy*/
    hi_uint32 ui_macindex; /* 外部phy对应的MAC索引*/
    hi_uchar8 uc_devname[HI_PHY_DEVNAME];
}hi_kernel_phy_s;
