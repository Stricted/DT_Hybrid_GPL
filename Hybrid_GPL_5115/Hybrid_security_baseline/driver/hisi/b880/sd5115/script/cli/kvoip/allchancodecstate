[settings]
functionname=hi_kernel_voip_query_allchancodecstate;---------不动它，罗成的命令行专用
attributenum=1;        ---------参数个数
attributeflag = 1 ;   //0:close 1:open---------是否显示参数

[parameter]
attribute=alias<message>       type<uint>  range<0x0,0xFFFFFFFF>  default<0>;

[help]
usage: 	allchancodecstate                      ; 查询编解码状态
	allchancodecstate -v kcmdtype 0xF200120E ; 查询编解码状态
       	allchancodecstate -v message 0x0 ; 	回调地址
