[settings]
functionname=hi_voip_cli_cfg_timeslot;  
attributenum=2;
attributeflag = 1 ;   //0:close 1:open

[parameter]
attribute=alias<chanid>     type<uint>   range<0x0,0xFFFFFFFF>   default<0>; /*通道号*/
attribute=alias<slot>       type<uint>   range<0x0,0xFFFFFFFF>   default<0>; /*HW时隙号*/

[help]
usage: cfg_timeslot               ; 配置DSP通道与HW通道的关系
       cfg_timeslot -v chanid 0   ; 通道号
       cfg_timeslot -v slot 0     ; HW时隙号

