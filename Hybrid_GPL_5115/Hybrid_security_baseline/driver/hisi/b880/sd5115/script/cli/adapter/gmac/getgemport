[settings]
functionname=hi_adapter_cli_gmac_get_gemport;
attributenum=5;
attributeflag = 1 ;

[parameter]
attribute=alias<fid>      type<ushort>   range<0x0,0xff>      default<1>;
attribute=alias<gemport>  type<ushort>   range<0x0,0xfff>     default<1>;
attribute=alias<tcont>   type<uchar>    range<0x0,0x8>       default<0>;
attribute=alias<res>      type<uchar>    range<0x0,0x0>       default<0>;
attribute=alias<res2>     type<ushort>   range<0x0,0x0>       default<0>;

[help]
typedef struct
{
    hi_ushort16 us_fid;
    hi_ushort16 us_gemport;
    hi_uchar8   uc_tcont;
    hi_uchar8   uc_res;
    hi_ushort16 us_res2;
}hi_gmac_fidmap_s;
