[settings]
functionname=hi_kernel_codec_hlp_port_flashhooktime;  
attributenum=3;
attributeflagflag = 1 ;   //0:close 1:open

[parameter]
attribute=alias<port_id>      		type<uint>   range<0,3>    default<0>;
attribute=alias<up_limit>     		type<uint>   range<0,500>  default<50>;
attribute=alias<down_limit>   		type<uint>   range<0,3000> default<300>;

[help]
usage: set_flashtime -v port_id 0 up_limit 0 down_limit 0
	