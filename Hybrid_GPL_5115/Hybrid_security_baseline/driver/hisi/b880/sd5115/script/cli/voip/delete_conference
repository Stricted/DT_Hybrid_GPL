[settings]
functionname=hi_voip_cli_delete_conference;  
attributenum=1;
attributeflag = 1 ;   //0:close 1:open

[parameter]
attribute=alias<confid>      type<uint>   range<0x0,0xFFFFFFFF>   default<0>; /*会议号*/

[help]
usage: delete_conference                ; 创建会议室      
       delete_conference -v confid 0    ; 会议号0
