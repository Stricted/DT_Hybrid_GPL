attribute=alias<em_ge1_en>       type<int>   range<0x0,0x01>  		default<0x00000000>;
attribute=alias<em_fe0_en>       type<int>   range<0x0,0x01>  		default<0x00000000>;
attribute=alias<em_fe1_en>       type<int>   range<0x0,0x01>  		default<0x00000000>;
attribute=alias<em_fe2_en>       type<int>   range<0x0,0x01>  		default<0x00000000>;
attribute=alias<em_pon_en>       type<int>   range<0x0,0x01>  		default<0x00000000>;
attribute=alias<em_cpu_en>       type<int>   range<0x0,0x01>  		default<0x00000000>;

[help]
/typedef struct 
* vlan 过滤表项 */
{
    uint    ui_vlan;
    
