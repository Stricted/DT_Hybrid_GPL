[settings]
functionname=0xF7002004;
attributenum=9;
attributeflag = 1 ;   //0:close 1:open

[parameter]
attribute=alias<module>          type<uint>  range<0x0,0xFFFFFFFF>  default<0x00000400>; /*全局模块*/
