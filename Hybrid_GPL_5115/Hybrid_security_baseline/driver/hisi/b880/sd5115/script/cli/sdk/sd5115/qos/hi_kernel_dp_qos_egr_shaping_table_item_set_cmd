[settings]
functionname = hi_kernel_dp_qos_egr_shaping_table_item_set_cmd;
attributenum = 6;
attributeflag= 1;

[parameter]
attribute=alias<egr>        type<uint>     range<0x0,0xFFFFFFFF>   default<0>;  
attribute=alias<adj_dir>    type<uint>     range<0x0,0xFFFFFFFF>   default<0>;  
attribute=alias<adj_len>    type<uint>     range<0x0,0xFFFFFFFF>   default<0>;  
attribute=alias<enable>     type<uint>     range<0x0,0xFFFFFFFF>   default<0>;  
attribute=alias<shp_type>   type<uint>     range<0x0,0xFFFFFFFF>   default<0>;  
attribute=alias<egr_rate>   type<uint>     range<0x0,0xFFFFFFFF>   default<0>;  


[help]
typedef struct
{
    HI_DP_QOS_EGR_E      em_egr;
    HI_DP_DIR_E          em_adj_dir;     /* 命名需讨论 */
    uint                 ui_adj_len;     /* 0 ~ 0x3F, 默认24 */
    HI_BOOL_E            em_enable;
    HI_DP_QOS_SHP_TYPE_E em_shp_type;    /* 只有CPU端口可以选择PPS,其他端口只能是BPS */
    uint                 ui_egr_rate;
} hi_dp_qos_egr_shaping_table_item_s;
