[settings]
functionname=hi_voip_cli_cfg_dtmf_transmode;  
attributenum=2;
attributeflag = 1 ;   //0:close 1:open

[parameter]
attribute=alias<chanid>      type<uint>   range<0x0,0xFFFFFFFF>    default<0x0>; /*通道号ui_chanid*/
attribute=alias<mode>        type<uint>   range<0x0,0xFFFFFFFF>   default<0x0>;  /*DTMF传输模式ui_mode*/


[help]
usage: cfg_dtmftransmode               ; 配置编解码      
       cfg_dtmftransmode -v chanid 0x0 ; 通道0
       cfg_dtmftransmode -v mode  0x0  ; DTMF传输模式：0
