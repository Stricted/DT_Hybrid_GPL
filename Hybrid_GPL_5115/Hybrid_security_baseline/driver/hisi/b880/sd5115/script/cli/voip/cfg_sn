[settings]
functionname=hi_voip_cli_rtpsn;  
attributenum=2;
attributeflag = 1 ;   //0:close 1:open

[parameter]
attribute=alias<chanid>     type<uint>   range<0,0xFFFFFFFF>      default<0>     ; /*通道号*/
attribute=alias<sn>         type<uint>   range<0,0xFFFFFFFF>      default<0>     ; initial RTP SN


[help]
usage: cfg_sn                ; 配置通道初始RTP SN
       cfg_sn -v chanid 2    ; 通道号2
       cfg_sn -v sn 12345    ; initial RTP SN 12345
