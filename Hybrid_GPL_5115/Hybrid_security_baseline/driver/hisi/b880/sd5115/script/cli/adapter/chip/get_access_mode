[settings]
functionname = hi_adapter_cli_chip_get_access_mode;
attributenum = 1;
attributeflag= 1;


[parameter]
attribute=alias<swmode>     type<uint>   range<0x0,0x2>       default<0>;

[help]
typedef enum
{
    HI_LSW_GLB_EXCHANG_MODE_E = 0,                  /*全交换模式*/
    HI_LSW_GLB_CONNECT_MODE_E,                      /*纯接入模式*/
    HI_LSW_GLB_CONNECT_EXCHANG_MODE_E,              /*接入交换模式*/
}hi_lsw_glb_sw_mode_e;

