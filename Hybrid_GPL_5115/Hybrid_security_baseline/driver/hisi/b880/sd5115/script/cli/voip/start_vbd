[settings]
functionname=hi_voip_cli_start_vbd;  
attributenum=7;
attributeflag = 1 ;   //0:close 1:open

[parameter]
attribute=alias<chanid>      	type<uint>     range<0x0,0xFFFFFFFF>   default<0>; /*通道号*/
attribute=alias<mode>   	    type<ushort>   range<0x0,0xFFFF>   default<0>; /*调制类型*/
attribute=alias<aulaw>   	    type<ushort>   range<0x0,0xFFFF>   default<0>; /*VBD的编解码方式*/
attribute=alias<tdmlevel>   	type<ushort>   range<0x0,0xFFFF>   default<12>;  /* TDM侧检测电平值*/
attribute=alias<iplevel>     	type<ushort>   range<0x0,0xFFFF>   default<12>;  /* IP侧检测电平值*/
attribute=alias<pritimer>   	type<ushort>   range<0x0,0xFFFF>   default<25>;   /*双向静音检测的主计时器*/
attribute=alias<sectimer>   	type<ushort>   range<0x0,0xFFFF>   default<16>;   /*双向静音检测的次计数器*/

[help]
usage: start_vbd                 ; 停止VBD传输   
       start_vbd -v chanid 0     ; channid
       start_vbd -v mode 0       ; /*调制类型*/
       start_vbd -v aulaw 0      ; /*VBD的编解码方式*/
       start_vbd -v tdmlevel 12  ; /* TDM侧检测电平值*/
       start_vbd -v iplevel  12  ; /* IP侧检测电平值*/
       start_vbd -v pritimer 25  ; /*双向静音检测的主计时器*/
       start_vbd -v sectimer 26  ; /*双向静音检测的次计数器*/

