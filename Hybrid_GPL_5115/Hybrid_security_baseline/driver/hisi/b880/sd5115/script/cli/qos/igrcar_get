[settings]
functionname = hi_kernel_qos_cmd_igr_car_get;
attributenum = 6;
attributeflag= 1;

[parameter]
attribute=alias<ifname>    type<string>  range<0,16>           default<nas0>;
attribute=alias<pri>       type<uint>    range<0,7>            default<0>;
attribute=alias<cir>       type<uint>    range<0,0xFFFFFFFF>   default<0xFFFFF>;
attribute=alias<cbs>       type<uint>    range<0,0xFFFFFFFF>   default<0x3FFFFFF>;
attribute=alias<pir>       type<uint>    range<0,0xFFFFFFFF>   default<0xFFFFF>;
attribute=alias<pbs>       type<uint>    range<0,0xFFFFFFFF>   default<0x3FFFFFF>;


[help]

typedef struct
{
    hi_uchar8   uc_ifname[16]; // 接口设备名
    hi_uint32   ui_pri;        // 报文优先级(802.1p)
    hi_uint32   ui_cir;        // car参数
    hi_uint32   ui_cbs; 
    hi_uint32   ui_pir; 
    hi_uint32   ui_pbs; 
}hi_kernel_qos_api_igr_car_s; 

