{
    hi_uchar8   uc_valid;
    hi_uchar8   uc_ageen;
    hi_ushort16 us_vlan;
    hi_uchar8   uc_mac[6];
    hi_uchar8   uc_untag;
    hi_uchar8   uc_resv;
    hi_lsw_port_e   em_egr;
}hi_lsw_mac_item_s;

