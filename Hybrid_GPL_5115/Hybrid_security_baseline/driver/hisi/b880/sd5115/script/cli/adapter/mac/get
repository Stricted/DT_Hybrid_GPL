attributeflag= 1;
[parameter]
attribute=alias<valid>             type<uchar>       range<0x0,0x1>                      default<0>;
attribute=alias<ageen>               type<uchar>      range<0x0,0x1>                      default<0>;
attribute=alias<vlan>              type<ushort>     range<0x0,0xfff>                    default<0>;
attribute=alias<mac>               type<mac>                                            default<00:00:00:00:00:00>;
attribute=alias<untag>             type<uchar>>     range<0x0,0x1>                      default<0>;
attribute=alias<resv>              type<uchar>      range<0x0,0x0>                      default<0>;
attribute=alias<port>              type<uint>       range<0x0,0x7>                      default<0>;

[help]
