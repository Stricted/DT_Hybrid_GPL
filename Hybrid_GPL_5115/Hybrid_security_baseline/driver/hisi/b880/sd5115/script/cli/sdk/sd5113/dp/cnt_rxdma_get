attribute=alias<fe4_rx>       		type<uint>   range<0x0,0xFFFFFFFF>  default<0x00000000>;
attribute=alias<pon_rx>       		type<uint>   range<0x0,0xFFFFFFFF>  default<0x00000000>;
attribute=alias<cpu_rx>       		type<uint>   range<0x0,0xFFFFFFFF>  default<0x00000000>;
