attribute=alias<fe1_drop>       	type<uint>   range<0x0,0xFFFFFFFF>  default<0x00000000>;
attribute=alias<fe2_drop>       	type<uint>   range<0x0,0xFFFFFFFF>  default<0x00000000>;
attribute=alias<fe3_drop>       	type<uint>   range<0x0,0xFFFFFFFF>  default<0x00000000>;
attribute=alias<fe4_drop>       	type<uint>   range<0x0,0xFFFFFFFF>  default<0x00000000>;
attribute=alias<pon_drop>       	type<uint>   range<0x0,0xFFFFFFFF>  default<0x00000000>;
attribute=alias<cpu_drop>       	type<uint>   range<0x0,0xFFFFFFFF>  default<0x00000000>;
attribute=alias<bm_malloc_suc>  	type<uint>   range<0x0,0xFFFFFFFF>  default<0x00000000>;
attribute=alias<bm_malloc_fail> 	type<uint>   range<0x0,0xFFFFFFFF>  default<0x00000000>;
attribute=alias<fra_head_tx>    	type<uint>   range<0x0,0xFFFFFFFF>  default<0x00000000>;
