[settings]
functionname=0xF7002004;
attributenum=13;
attributeflag = 1 ;   //0:close 1:open

[parameter]
attribute=alias<module>       		type<uint>   range<0x0,0xFFFFFFFF>  default<0x00000D02>; /*全局模块*/
attribute=alias<fe0_rx>       		type<uint>   range<0x0,0xFFFFFFFF>  default<0x00000000>;
