<?xml version="1.0" encoding="UTF-8"?>
<config>
<!-- global模块参数 -->
<!--  拨号成功自动打开指定主页，配置为空则不打开 --> 
<homepage></homepage>
<!--  默认语言 --> 
<default_language>en-us</default_language>

<!-- 产品系列E:表示MBB产品；B表示无线路由产品-->
<product_type>B</product_type>
<!-- WebUI参数-->
<!--  WebUI版本号 --> 
<version>100.001.994.18.117</version>
<!--  自动消失的对话框消失的时间 --> 
<dialogdisapear>3000</dialogdisapear>
<!--  tips自动消失的时间 --> 
<tip_disapear>3000</tip_disapear>
<!--  页面中自动刷新数据的时间 --> 
<update_interval>6000</update_interval> 
    <!-- 是否支持AP station功能。 取值范围[0,1] -->
    <ap_station_enabled>0</ap_station_enabled>

    <!-- 是否支持multi_ssid功能。 取值范围[0,1] -->
    <multi_ssid_enabled>0</multi_ssid_enabled>
    
    <!-- 是否需要登录-->
    <login>1</login>

    <!-- profile管理页面上是否显示autoapn-->
    <autoapn_enabled>0</autoapn_enabled>

    <!-- 是否需要页脚-->
    <footer>1</footer>
    
     <!-- 是否有电池-->
    <battery_enabled>0</battery_enabled>
    
     <!-- 是否有跳转至主页的continue按钮，取值范围[0,1]-->
    <continue_button>1</continue_button>
    
    <!-- 输入主页地址时，是否进行特殊情况重定向-->
    <special_redirect>0</special_redirect>

    <!--主页是否进行特殊情况重定向，取值范围[0,1]-->
    <home_redirect>0</home_redirect>
    
    <!--主页是否显示广告，取值范围[0,1]-->
    <adcontent_enable>0</adcontent_enable>

    <!--是否支持IPv6，取值范围[0,1]-->
    <ipv6_enabled>1</ipv6_enabled>
	
	 <!--是否显示wireline wan,取值范围[0,1]-->
    <wirelinewan_enable>1</wirelinewan_enable>

    <!-- hotlinks链接定义-->
    <hotlinks>
        <enable>1</enable>
    <items>
            <item></item>
            <item></item>
        <item></item>
        </items>
    </hotlinks>

    <!-- 连接状态显示-->
    <connection>
        <enable>1</enable>
        <connectionstatus>1</connectionstatus>
    </connection>

    <!-- 页面菜单定义-->
    <menu>
        <home>home</home>
        <quicksetup>quicksetup</quicksetup>
        <sms>
           <inbox>sms</inbox>
           <sent>smssent</sent>
           <drafts>smsdrafts</drafts>
           <sms_center_number>messagesettings</sms_center_number>
        </sms>
        <!--sharing>sdcardsharing</sharing-->
        
        
        <!--<ussd>ussd</ussd>-->
        <settings>
            <!--quicksetup>setting</quicksetup-->
            <dialup>
                <connections>setting</connections>
                <profilesmgr>profilesmgr</profilesmgr>
				<wan>wan</wan>
                <!--mobilenetworksettings>mobilenetworksettings</mobilenetworksettings-->
                
            </dialup>
                                
            <wlan>
                <wlansettings>wlansettings</wlansettings>
                <wlanbridge>wlanbridge</wlanbridge>
                <wlanmacfilter>wlanmacfilter</wlanmacfilter>
                <wps>wps</wps>
            </wlan>
            <security>
                <pincodemanagement>pincodemanagement</pincodemanagement>
                <pincodeautovalidate>pincodeautovalidate</pincodeautovalidate>
                <firewallswitch>firewallswitch</firewallswitch>
                <lanipfilter>lanipfilter</lanipfilter>
                <lanmacfilter>lanmacfilter</lanmacfilter>
                <urlfilter>urlfilter</urlfilter>
                <virtualserver>virtualserver</virtualserver>
                <specialapplication>specialapplication</specialapplication>
                <dmzsettings>dmzsettings</dmzsettings>
                <sipalgsettings>sipalgsettings</sipalgsettings>
                <upnp>upnp</upnp>
                <!--nat>nat</nat-->
                <aclsettings>aclsettings</aclsettings>
            </security>
            
            <system>
                <deviceinformation>deviceinformation</deviceinformation>
                <wanstatus>wanstatus</wanstatus>
                <lanstatus>lanstatus</lanstatus>
                <modifypassword>modifypassword</modifypassword>
                <restore>restore</restore>
                <reboot>reboot</reboot>
				<!--autoreboot>autoreboot</autoreboot-->
            </system>

            <dhcpmenu>
                <ipv4dhcp>dhcp</ipv4dhcp>
                <ipv6dhcp>ipv6dhcp</ipv6dhcp>
                <slaac>slaac</slaac>
                <staticaddr>staticaddr</staticaddr>
            </dhcpmenu>

            <advancedmenu>
                <ddns>ddns</ddns>
                <sntp>sntp</sntp>
                <!--tr069>tr069</tr069-->
                <!--tr111>tr111</tr111-->
                <samba>samba</samba>
                <!--ftp>ftp</ftp-->
                <!--antenna>antenna</antenna-->
                <staticroute>staticroute</staticroute>
                <!--<ipsecvpn>ipsecvpn</ipsecvpn>-->
                <dms>dms</dms>
                <clip>clip</clip>
            </advancedmenu>
        </settings>

        <maintenance>
            <logview>maintenance</logview>
            <configuration>configurationnew</configuration>
            <diagnosis>diagnosis</diagnosis>
            <onekeydiag>onekeydiag</onekeydiag>
            <update>update</update>
        </maintenance>
    </menu>
	
