[settings]
functionname = hi_kernel_voip_config_tdm_samplerate;
attributenum = 2;
attributeflag = 1 ;

[parameter]
attribute=alias<sample_rate>       type<uint>    range<0x0,0x1>  default<0x1>; /*采样率：0:8KHZ;1:16KHZ*/
attribute=alias<handler>           type<uint>    range<0x0,0xFFFFFFFF>  default<0x00000000>; /*用户回调句柄*/

[help]
usage: sample_rate 0:8KHZ;1:16KHZ                ; 
