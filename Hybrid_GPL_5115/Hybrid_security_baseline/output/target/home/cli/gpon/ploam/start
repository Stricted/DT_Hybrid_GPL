[settings]
functionname=hi_kernel_ploam_start;  
attributenum=1;
attributeflag = 1 ;   //0:close 1:open

[parameter]
attribute=alias<sn>           type<binary> range<0,8>             default<0x68 0x77 0x68 0x77 0x01 0x02 0x01 0x02>;

[help]
usage: start								  ; 初始化ploam,启动ploam注册线程开始注册			

