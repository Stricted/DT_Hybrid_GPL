attribute=alias<em_mir_dport>      		type<int>      range<0x0,0x08>  	  default<0x00000000>;
attribute=alias<ui_vlan>       	   		type<uint>     range<0,4095>  		  default<1>;
attribute=alias<em_cfi>       	   		type<int>      range<0x0,0x01>  	  default<0x00000000>;
attribute=alias<ui_pri>       	   		type<uint>     range<0,7>  			  default<0x00000000>;
attribute=alias<em_tpid_modify_en>      type<int>      range<0x0,0x01>  	  default<0x00000000>;
attribute=alias<us_uni_stpid_value>     type<ushort>   range<0x0,0xFFFF>  	  default<0x8100>;
attribute=alias<us_uni_ctpid_value>     type<ushort>   range<0x0,0xFFFF>  	  default<0x8100>;
