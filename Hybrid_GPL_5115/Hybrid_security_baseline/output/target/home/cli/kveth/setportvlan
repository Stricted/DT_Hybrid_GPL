[settings]
functionname=hi_kernel_vif_cmdtype_vlan_set;  
attributenum=5;
attributeflag = 1 ;   //0:close 1:open

[parameter]
attribute=alias<ifname>       type<string>   range<0x0,32>           default<eth0>;
attribute=alias<vlanflag>     type<uchar>    range<0x0,0xFF>         default<0>;
attribute=alias<resv>         type<binary>   range<0x0,3>            default<0>;
attribute=alias<defvlan>      type<ushort>   range<0x0,0xFFFF>       default<1>;
attribute=alias<deftpid>      type<ushort>   range<0x0,0xFFFF>       default<0x8100>;

[help]
typedef struct
{
    hi_uchar8   uc_ifname[HI_KERNEL_VIF_NAMELEN];
    hi_uchar8   uc_vlanflag;  /*vlan处理动作 HI_KERNEL_NETWORK_VLAN_ACTION_VLAN*/  
    hi_uchar8   uc_resv[3];
    hi_ushort16 us_defvlan;   /*缺省0为没有激活*/
    hi_ushort16 us_deftpid;   /*缺省0x8100*/
}hi_kernel_vif_cmdtype_vlan_s;