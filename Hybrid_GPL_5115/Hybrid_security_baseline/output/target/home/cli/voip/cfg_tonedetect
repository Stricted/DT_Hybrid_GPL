[settings]
functionname=hi_voip_cli_cfg_tonedetect;  
attributenum=4;
attributeflag = 1 ;   //0:close 1:open

[parameter]
attribute=alias<chanid>       type<uint>     range<0x0,0xFFFFFFFF>      default<0x0>; /*通道号*/

attribute=alias<direction>    type<uint>     range<0x0,0xFFFFFFFF>      default<0x1>;     /*TD检测方向*/
attribute=alias<mask[0]>      type<uint>     range<0x0,0xFFFFFFFF>      default<0xffffffff>;  /*TD掩码*/
attribute=alias<mask[1]>      type<uint>     range<0x0,0xFFFFFFFF>      default<0x7fffffff>;  /*TD掩码*/


[help]
usage: cfg_tonedetect                        ; 配置TD检测开关    
       cfg_tonedetect -v chanid 0x0          ; 通道号
       cfg_tonedetect -v direction 0x1       ; TD检测方向
       cfg_tonedetect -v mask[0] 0xffffffff  ; TD掩码
       cfg_tonedetect -v mask[1] 0x7fffffff  ; TD掩码
