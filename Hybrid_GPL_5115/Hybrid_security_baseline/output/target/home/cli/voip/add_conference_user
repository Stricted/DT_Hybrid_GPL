[settings]
functionname=hi_voip_cli_add_conference_user;  
attributenum=3;
attributeflag = 1 ;   //0:close 1:open

[parameter]
attribute=alias<chanid>       type<uint>   range<0x0,0xFFFFFFFF>   default<0>; /*通道号*/
attribute=alias<confid>       type<uint>   range<0x0,0xFFFFFFFF>   default<0>; /*confid*/
attribute=alias<direction>    type<uint>   range<0x0,0xFFFFFFFF>   default<1>; /*direction*/

[help]
usage: add_conference_user                ; 添加会议室成员    
       add_conference_user -v chanid 0    ; 通道号
       add_conference_user -v confid 0    ; 会议ID
       add_conference_user -v direction 0 ; 会议成员方向