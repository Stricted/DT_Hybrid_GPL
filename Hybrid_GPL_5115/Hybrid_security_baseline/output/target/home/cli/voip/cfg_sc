[settings]
functionname=hi_voip_cli_cfg_vadcngsc;  
attributenum=4;
attributeflag = 1 ;   //0:close 1:open

[parameter]
attribute=alias<chanid>    type<uint>     range<0x0,0xFFFFFFFF>      default<0x0>; /*通道号*/

attribute=alias<cng>       type<uint>     range<0x0,0xFFFFFFFF>      default<0x1>; /*CNG使能*/
attribute=alias<vad>       type<uint>     range<0x0,0xFFFFFFFF>      default<0x1>; /*VAD使能*/
attribute=alias<sc>        type<uint>     range<0x0,0xFFFFFFFF>      default<0x1>; /*静音压缩使能*/


[help]
usage: cfg_sc                  ; 配置配置静音压缩参数
       cfg_sc -v chanid 0      ; 通道号
       cfg_sc -v cng 0x1       ; CNG使能
       cfg_sc -v vad 0x1       ; VAD使能
       cfg_sc -v sc  0x1       ; 静音压缩使能
       