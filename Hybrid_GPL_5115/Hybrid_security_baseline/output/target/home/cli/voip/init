[settings]
functionname=hi_voip_cli_init;  
attributenum=2;
attributeflag = 1 ;   //0:close 1:open

[parameter]
attribute=alias<addr>       type<uint>   range<0x0,0xFFFFFFFF>  default<0x80a00000>; /*内存地址*/
attribute=alias<path>       type<string> range<0,32>            default</usr/bin/zsp_min.out>


[help]
usage: init                         ; VoIP模块初始化      
       init -v addr 0x86e00000      ; 为VoIP模块分配的内存(物理地址)
       init -v path /usr/bin/zsp_min.out
