[settings]
functionname=hi_voip_cli_test_openchan;  
attributenum=2;
attributeflag = 1 ;   //0:close 1:open

[parameter]
attribute=alias<chanid0>    type<uint>   range<0,3>  default<0>; /*通道号0*/
attribute=alias<chanid1>    type<uint>   range<0,3>  default<1>; /*通道号1*/


[help]
usage: test_talk                            ; 启动放音
       test_talk -v chanid0 1 chanid1 2     ; channel 1和channel 2通话
