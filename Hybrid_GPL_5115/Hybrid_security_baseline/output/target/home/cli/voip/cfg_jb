[settings]
functionname=hi_voip_cli_cfg_jb;  
attributenum=5;
attributeflag = 1 ;   //0:close 1:open

[parameter]
attribute=alias<chanid>    type<uint>     range<0x0,0xFFFFFFFF>       default<0x0>;    /*通道号*/
                                                                                       
attribute=alias<static>    type<uint>     range<0x0,0xFFFFFFFF> 	  	default<0x0>;    /*JB模式*/
attribute=alias<minsize>   type<uint>     range<0x0,0xFFFFFFFF> 	  	default<0x6>;    /*JB最小深度*/
attribute=alias<maxsize>   type<uint>     range<0x0,0xFFFFFFFF> 	  	default<0xc8>;   /*JB最大深度*/
attribute=alias<initsize>  type<uint>     range<0x0,0xFFFFFFFF> 	  	default<0x6>;    /*JB初始深度*/


[help]
usage: cfg_jb								   ; 配置jitter buffer参数
	     cfg_jb -v chanid 0x0    ; 通道号
       cfg_jb -v static 0x0    ; JB模式:0-动态JB
       cfg_jb -v minsize 0x6   ; JB最小深度为6
       cfg_jb -v maxsize 0xc8  ; JB最大深度为200
       cfg_jb -v initsize 0x6  ; JB初始深度为6