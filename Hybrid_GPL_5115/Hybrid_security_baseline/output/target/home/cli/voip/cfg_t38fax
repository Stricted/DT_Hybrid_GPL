[settings]
functionname=hi_voip_cli_cfg_t38fax;  
attributenum=2;
attributeflag = 1 ;   //0:close 1:open

[parameter]
attribute=alias<chanid>      type<uint>   range<0x0,0xFFFFFFFF>   default<0>; /*通道号*/
attribute=alias<ecmmode>     type<uint>   range<0x0,0xFFFFFFFF>   default<0>; /*ECMMODE*/
attribute=alias<framemode>   type<uint>   range<0x0,0xFFFFFFFF>   default<0>; /*framemode*/

[help]
usage: cfg_t38fax                ; 配置t38传输    
       cfg_t38fax -v chanid 0    ; 通道号
       cfg_t38fax -v ecmmode 0   ; ECMMODE
       cfg_t38fax -v framemode 0 ; framemode
