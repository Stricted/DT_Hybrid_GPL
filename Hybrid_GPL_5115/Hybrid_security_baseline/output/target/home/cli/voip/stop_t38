[settings]
functionname=hi_voip_cli_stop_t38;  
attributenum=1;
attributeflag = 1 ;   //0:close 1:open

[parameter]
attribute=alias<chanid>       type<uint>   range<0x0,0xFFFFFFFF>   default<0>; /*通道号*/
attribute=alias<codec>        type<uint>   range<0x0,0xFFFFFFFF>   default<0>; /*编解码*/

[help]
usage: stop_t38                ; 停止T38传输   
       stop_t38 -v chanid 0    ; channid
       stop_t38 -v codec 0     ; codec

