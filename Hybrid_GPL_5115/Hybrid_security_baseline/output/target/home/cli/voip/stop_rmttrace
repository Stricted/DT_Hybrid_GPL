[settings]
functionname=hi_voip_cli_stop_rmttrace;  
attributenum=1;
attributeflag = 1 ;   //0:close 1:open

[parameter]
attribute=alias<chanid>      type<uint>   range<0x0,0xFFFFFFFF>   default<0>; /*通道号*/

[help]
usage: stop_rmttrace             ;停止远程抓包 
       stop_rmttrace -v chanid 0 ; channel id

