[settings]
functionname=hi_voip_cli_create_connection;  
attributenum=2;
attributeflag = 1 ;   //0:close 1:open

[parameter]
attribute=alias<chanid>      type<uint>   range<0x0,0xFFFFFFFF>   default<0>; /*通道号*/
attribute=alias<codec>       type<uint>   range<0x0,0xFFFFFFFF>   default<0x00>; /*编解码类型：G711*/


[help]
usage: create_connection                ; 创建连接      
       create_connection -v chanid 2    ; 通道号2 
       create_connection -v codec  1    ；编解码 G.711 Mu, 参见枚举 hi_voip_codec_e
