[settings]
functionname=hi_voip_cli_stop_dtmf;  
attributenum=2;
attributeflag = 1 ;   //0:close 1:open

[parameter]
attribute=alias<chanid>     type<uint>   range<0x0,0xFFFFFFFF>   default<0>; /*通道号*/
attribute=alias<direction>  type<uint>   range<0x0,0xFFFFFFFF>   default<1>; /*放音方向*/

[help]
usage: stop_dtmf                  ;停止DTMF放音   
       stop_dtmf -v chanid 0      ; channel id
       stop_dtmf -v direction 0   ; 放音方向
