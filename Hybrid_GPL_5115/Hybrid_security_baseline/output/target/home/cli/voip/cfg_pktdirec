[settings]
functionname=hi_voip_cli_cfg_pktdirec;  
attributenum=2;
attributeflag = 1 ;   //0:close 1:open

[parameter]
attribute=alias<chanid>       type<uint>   range<0x0,0xFFFFFFFF>    default<0x0>;    /*通道号ui_chanid*/
attribute=alias<direc>        type<uint>   range<0x0,0xFFFFFFFF>    default<0x0>;    /*媒体流方向ui_direc*/


[help]
usage: cfg_pktdirec               ; 配置编解码      
       cfg_pktdirec -v chanid 0x0 ; 通道0
       cfg_pktdirec -v direc  0x0 ; 媒体流方向：0
