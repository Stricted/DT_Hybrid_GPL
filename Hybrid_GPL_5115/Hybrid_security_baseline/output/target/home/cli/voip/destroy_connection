[settings]
functionname=hi_voip_cli_destroy_connection;  
attributenum=1;
attributeflag = 1 ;   //0:close 1:open

[parameter]
attribute=alias<chanid>     type<uint>   range<0,0xFFFFFFFF>  default<0>; /*通道号*/


[help]
usage: destroy_connection              ; 删除连接       
       destroy_connection -v chanid 2  ; 删除通道2连接
