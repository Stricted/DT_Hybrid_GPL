[settings]
functionname=hi_tsenser_attr_set;  
attributenum=1;
attributeflag = 1 ;   //0:close 1:open

[parameter]
attribute=alias<en>         type<uint>     range<0x0,0xFFFFFFFF> 	default<0>;


[help]

typedef struct
{
    HI_BOOL_E em_temp_sensor_en;                  /*温度传感器使能*/              
}hi_tsenser_attr_s;

/* 用于某功能属性的使能 */
typedef enum
{
    HI_FALSE_E  = 0   , 
    HI_TRUE_E     
}HI_BOOL_E;

